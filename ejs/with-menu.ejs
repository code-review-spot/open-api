<div class="col-md-3">
	<div class="page-header">
		<h1>Routes</h1>
	</div>
	<ul class="nav nav-pills nav-stacked">
		<% for(var i=0; i<routes.length; i++){ %>
			<li <% if(routes[i].isCurrentPage){ %>class="active" } <% } %> >
				<a href="<%- routes[i].url%>">
					<%- routes[i].name %>
				</a>
			</li>
		<% } %>
	</ul>	
</div>
<div class="col-md-9">
	<%- __yeild %>
</div>